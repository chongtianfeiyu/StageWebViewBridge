<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<application xmlns="http://ns.adobe.com/air/application/2.7">
   
    <id>@id@</id>
    <versionNumber>@majorVersion@.@minorVersion@.@buildNumber@</versionNumber>
    <filename>@filename@</filename>
    <description/>
    <name>@name@</name>
    <copyright/>
   
    <initialWindow>
        <content>@filename@.swf</content>
        <systemChrome>standard</systemChrome>
        <transparent>false</transparent>
        <visible>true</visible>
        <fullScreen>true</fullScreen>
        <aspectRatio>landscape</aspectRatio>
        <renderMode>gpu</renderMode>
        <autoOrients>true</autoOrients>
    </initialWindow>
   
    <icon> 
        <image29x29>icons/29.png</image29x29> 
        <image57x57>icons/57.png</image57x57> 
        <image512x512>icons/512.png</image512x512> 
    </icon>
    <customUpdateUI>false</customUpdateUI>
    <allowBrowserInvocation>false</allowBrowserInvocation>

    <iPhone>
		<InfoAdditions>
            <![CDATA[
               	<key>UIDeviceFamily</key>
               	<array>
                   <string>2</string>
               	</array>
                <key>UIApplicationExitsOnSuspend</key> 
				<true/>
				<key>UIFileSharingEnabled</key>
				<true/>	               
           ]]>
        </InfoAdditions>
    </iPhone>

</application>

